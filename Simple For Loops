#include <iostream>
using namespace std;

int main() {
    // Print even values between 0 and 10
    for (int i = 0; i <= 10; i++) {
        if (i % 2 == 0)
            cout << i << endl;
    }
 cout<<endl;
    // Print "SIT" 5 times
    for (int i = 1; i <= 5; i++) {
        cout << "SIT" << endl;
    }

    return 0;
}

